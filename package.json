{
    "name": "dxdao-landing-page",
    "version": "0.2.2",
    "description": "DXdao landingpage dxdao.eth.link",
    "scripts": {
        "start": "./scripts/start.sh",
        "dev": "./scripts/dev.sh",
        "build": "node scripts/build.js",
        "ipfs-deploy": "node scripts/build.js && ipfs-deploy build --pinner pinata --no-open --no-clipboard"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/fMercury/dxdao-landing-page.git"
    },
    "authors": "John Kelleher <john.kelleher@levelk.io>, Thomas Pulber <corkus@pm.me>, Thomas Spofford <tspofford@pm.me>, Augusto Lemble <me@augustol.com>",
    "bugs": {
        "url": "https://github.com/fMercury/dxdao-landing-page/issues"
    },
    "homepage": "./",
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "jest": {
        "roots": [
            "<rootDir>/src"
        ],
        "collectCoverageFrom": [
            "src/**/*.{js,jsx,ts,tsx}",
            "!src/**/*.d.ts"
        ],
        "setupFiles": [
            "react-app-polyfill/jsdom"
        ],
        "setupFilesAfterEnv": [],
        "testMatch": [
            "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
            "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
        ],
        "testEnvironment": "jest-environment-jsdom-fourteen",
        "transform": {
            "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
            "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
            "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
        },
        "transformIgnorePatterns": [
            "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
            "^.+\\.module\\.(css|sass|scss)$"
        ],
        "modulePaths": [],
        "moduleNameMapper": {
            "^react-native$": "react-native-web",
            "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
        },
        "moduleFileExtensions": [
            "web.js",
            "js",
            "web.ts",
            "ts",
            "web.tsx",
            "tsx",
            "json",
            "web.jsx",
            "jsx",
            "node"
        ],
        "watchPlugins": [
            "jest-watch-typeahead/filename",
            "jest-watch-typeahead/testname"
        ]
    },
    "babel": {
        "presets": [
            "react-app"
        ],
        "plugins": [
            [
                "@babel/plugin-proposal-decorators",
                {
                    "legacy": true
                }
            ]
        ]
    },
    "lint-staged": {
        "src/**/*.{js,jsx,ts,tsx}": [
            "tslint --project tsconfig.json -c tslint.json --fix",
            "prettier --write",
            "git add"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "dependencies": {
        "@babel/core": "7.6.0",
        "@babel/plugin-proposal-decorators": "^7.6.0",
        "@babel/plugin-transform-react-jsx": "^7.3.0",
        "@reach/dialog": "^0.8.5",
        "@svgr/webpack": "4.3.2",
        "@types/jest": "^25.1.1",
        "@types/node": "^13.7.0",
        "@types/react": "^16.9.19",
        "@types/react-dom": "^16.9.5",
        "@typescript-eslint/eslint-plugin": "^2.2.0",
        "@typescript-eslint/parser": "^2.2.0",
        "@uprtcl/ethereum-provider": "0.0.7-alpha.49",
        "@uprtcl/evees": "0.0.7-alpha.49",
        "@uprtcl/graphql": "0.0.7-alpha.49",
        "@uprtcl/ipfs-provider": "0.0.7-alpha.49",
        "@uprtcl/micro-orchestrator": "0.0.7-alpha.49",
        "@uprtcl/multiplatform": "0.0.7-alpha.49",
        "@uprtcl/cortex": "0.0.7-alpha.49",
        "@uprtcl/access-control": "0.0.7-alpha.49",
        "@uprtcl/documents": "0.0.7-alpha.49",
        "@uprtcl/wikis": "0.0.7-alpha.49",
        "babel-eslint": "10.0.3",
        "babel-jest": "^24.9.0",
        "babel-loader": "8.0.6",
        "babel-plugin-named-asset-import": "^0.3.4",
        "babel-preset-react-app": "^9.0.2",
        "bignumber.js": "^9.0.0",
        "camelcase": "^5.2.0",
        "case-sensitive-paths-webpack-plugin": "2.2.0",
        "copy-to-clipboard": "^3.3.1",
        "css-loader": "2.1.1",
        "dotenv": "^8.2.0",
        "dotenv-expand": "5.1.0",
        "file-loader": "3.0.1",
        "hdkey": "^1.1.1",
        "html-webpack-plugin": "4.0.0-beta.5",
        "identity-obj-proxy": "3.0.0",
        "ipfs-deploy": "^7.14.0",
        "is-wsl": "^1.1.0",
        "isomorphic-fetch": "^2.2.1",
        "jazzicon": "^1.5.0",
        "loglevel": "^1.6.4",
        "mini-css-extract-plugin": "0.8.0",
        "mobx": "^5.14.0",
        "mobx-react": "^6.1.3",
        "moment": "^2.24.0",
        "node-sass": "^4.12.0",
        "optimize-css-assets-webpack-plugin": "5.0.3",
        "pnp-webpack-plugin": "1.5.0",
        "polished": "^3.4.4",
        "postcss-flexbugs-fixes": "4.1.0",
        "postcss-loader": "3.0.0",
        "postcss-normalize": "7.0.1",
        "postcss-preset-env": "6.7.0",
        "postcss-safe-parser": "4.0.1",
        "promievent": "^0.1.3",
        "promise": "^8.0.3",
        "qrcode.react": "^1.0.0",
        "react": "^16.10.2",
        "react-app-polyfill": "^1.0.4",
        "react-chartjs-2": "^2.9.0",
        "react-dev-utils": "^9.1.0",
        "react-device-detect": "^1.11.14",
        "react-dom": "^16.10.2",
        "react-feather": "^2.0.3",
        "react-material-ui-form-validator": "^2.0.9",
        "react-meta-tags": "^0.7.4",
        "react-router-dom": "^5.1.2",
        "react-spring": "^8.0.27",
        "react-use-gesture": "^7.0.4",
        "resolve": "^1.12.0",
        "resolve-url-loader": "3.1.0",
        "sass-extract": "^2.1.0",
        "sass-extract-js": "^0.4.0",
        "sass-extract-loader": "^1.1.0",
        "sass-loader": "7.2.0",
        "semver": "6.3.0",
        "strip-hex-prefix": "^1.0.0",
        "style-loader": "1.0.0",
        "styled-components": "^4.4.1",
        "terser-webpack-plugin": "1.4.1",
        "ts-pnp": "1.1.4",
        "tslint": "^6.0.0",
        "tslint-config-prettier": "^1.18.0",
        "tslint-react": "^4.2.0",
        "typescript": "3.6.5",
        "url-loader": "2.1.0",
        "webpack": "4.41.0",
        "webpack-dev-server": "3.2.1",
