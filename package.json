        "workbox-webpack-plugin": "4.3.1"
    },
    "devDependencies": {
        "eslint": "^6.8.0",
        "eslint-config-react-app": "^5.0.2",
        "eslint-loader": "3.0.2",
        "eslint-plugin-flowtype": "3.13.0",
        "eslint-plugin-import": "2.18.2",
        "eslint-plugin-jsx-a11y": "6.2.3",
        "eslint-plugin-react": "7.14.3",
        "eslint-plugin-react-hooks": "^1.6.1",
        "fs-extra": "^7.0.1",
        "husky": "^4.2.5",
        "lint-staged": "^10.1.5",
        "prettier": "^2.0.4",
        "pretty-quick": "^2.0.1"
    }
}
